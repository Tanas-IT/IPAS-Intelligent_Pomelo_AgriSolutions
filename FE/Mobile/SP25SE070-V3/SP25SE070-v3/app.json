{
  "expo": {
    "name": "Ipas",
    "slug": "SP25SE070-v3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo-removebg-preview1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo-removebg-preview.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.laggg.SP25SE070",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to take photos or record video.",
        "NSMicrophoneUsageDescription": "We need access to your microphone to record audio.",
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo-removebg-preview1.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.laggg.SP25SE070"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/BalsamiqSans-Regular.ttf"]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "4af581fe-afec-49db-92c6-cd9b68a6dd2f"
      }
    }
  }
}
